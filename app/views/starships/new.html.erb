<h1>Add New Starship</h1>

<div>
    <%= form_for(@starship)  do |f| %>

        <%= f.label(:name, "Starship Name") %>
        <%= f.text_field :name %>

        <%= f.label :ship_class, "Ship Class" %>
        <%= f.text_field :ship_class %>

        <%= f.label :location %>
        <%= f.text_field :location %>

        <%= f.submit "Create Ship" %>
    <% end %>
</div>
