<h1> Add New Crew Member </h1>

<%= form_for [@starship, @crew_member] do |f| %>
  <%= f.label :first_name%>
  <%= f.text_field :first_name%>

  <%= f.label :last_name%>
  <%= f.text_field :last_name%>

  <%= f.label :specialty_division%>
  <%= f.text_field :specialty_division%>

  <%= f.submit "Add Crew Member"%>

<% end %>